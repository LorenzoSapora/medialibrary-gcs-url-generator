        "laravel/framework": "5.5.0",
        "spatie/laravel-medialibrary": "^7.12",
        "php": "^7.0",
        "superbalist/laravel-google-cloud-storage": "^2.2"
    },
    "license": "MIT",
    "homepage": "https://github.com/LorenzoSapora/GcsUrlGenerator",
    "authors": [
        {
            "name": "Lorenzo Sapora",
            "email": "lorenzo.sapora@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
